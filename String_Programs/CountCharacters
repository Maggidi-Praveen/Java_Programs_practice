public class CountCharacters {
	public static void main(String[] args) {
		String s="&2gtD";
		int cU=0,cL=0,cD=0,cS=0;
		
		for (int i = 0; i <= s.length()-1; i++) {
			char ch=s.charAt(i);
			if(ch>='a'&&ch<='z') {
				cL++;
			}
			else if (ch>='A'&&ch<='Z') {
				cU++;
			}
			else if (ch>='0'&&ch<='9') {
				cD++;
			}
			else {
				cS++;
			}
		}
		System.out.println(cL);
		System.out.println(cU);
		System.out.println(cD);
		System.out.println(cS);
	}
}
